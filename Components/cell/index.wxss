.container{
    height:60rpx;
    border: 2rpx solid #333333;
    padding: 2rpx;
    box-sizing: border-box;
}
/** 这里并没有给自定义组件使用宽度，而是由外部的container 布局使用flex布局 **/
.inner-container{
    height: 100%;
    padding-left: 15rpx;
    padding-right: 15rpx;
    display:flex;
    flex-direction:row;
    align-items: center;
    background-color: #ffffff;
    font-size: 24rpx;
}
.forbidden{
    border:2rpx dashed #bbbbbb;
    color:#bbbbbb;
}
.s-outer{
    border: 2rpx solid #157658;
    color:#ffffff;
}
.s-inner{
    background-color: #157658;
}
